			<!-- 内容头部 -->
			<section class="content-header">
			<h1>
				邮箱内容
			</h1>
			<ol class="breadcrumb">
				<li><a href="${pageContext.request.contextPath}/index.jsp"><i
						class="fa fa-dashboard"></i> 首页</a></li>
				<li class="active">邮箱内容</li>
			</ol>
			</section>
			<!-- 内容头部 /-->

			<%-- 短信内容编辑区域 --%>
			<!-- Main content -->
			<section class="content">
				<div class="row">
					<div class="col-md-12">

						<div class="box box-success">
							<div class="box-header">
								<h3 class="box-title">邮箱内容编辑
								</h3>
							</div>
							<!-- /.box-header -->
							<div class="box-body pad">
								<form
										action="${pageContext.request.contextPath}/clues/pushEMail.do"
										method="post">
									<!-- 正文区域 -->
									<section class="content">
										<input type="text" style="display: none" class="form-control" name="email"
											   placeholder="客户邮箱" value="${sendSmsVo.email}">
										<input type="text" class="form-control" name="subject"
											   placeholder="请输入邮件主题" value="">
										<textarea class="textarea" name="content" placeholder="请输入邮箱内容" style="width: 100%; height: 200px; font-size: 14px; line-height: 18px; border: 1px solid #dddddd; padding: 10px;"></textarea>

										<!--订单信息/--> <!--工具栏-->
										<div class="box-tools text-center">
											<button type="submit" class="btn bg-maroon">保存</button>
											<button type="button" class="btn bg-default"
													onclick="history.back(-1);">返回</button>
										</div>
										<!--工具栏/--> </section>
									<!-- 正文区域 /-->
								</form>
							</div>
						</div>


					</div>
					<!-- /.col-->
				</div>
				<!-- ./row -->
			</section>
			<!-- /.content -->

